#include <iostream>
#include <string>
#include <algorithm>
#include <vector>
#include <cmath>

using namespace std;

void init(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
}

int main(){
	init();
	int n,cnt=0;
	cin>>n;
	while(true){
		int tmp=i*i;
		if(n>=tmp){
			n-=tmp;
			cnt++;
			i=floor(sqrt(n));
		}
		if(n==0) break;
	}

	cout<<cnt<<endl;

	return 0;
}
